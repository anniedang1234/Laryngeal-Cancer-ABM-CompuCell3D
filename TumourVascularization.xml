   <Plugin Name="Chemotaxis">
      
      <!-- You may repeat ChemicalField element for each chemical field declared in the PDE solvers -->
      <!-- Specification of chemotaxis properties of select cell types. -->
      <ChemicalField Name="S_VEGF">
         <ChemotaxisByType ChemotactTowards="Medium,P,N" Lambda="5000.0" Type="EC"/>
         <ChemotaxisByType ChemotactTowards="Medium,P,N" Lambda="5000.0" Type="NV"/>
      </ChemicalField>
      <ChemicalField Name="L_VEGF">
         <ChemotaxisByType ChemotactTowards="Medium,P,N" Lambda="5000.0" Type="NV"/>
      </ChemicalField>
   </Plugin>
   
   <Plugin Name="ConnectivityGlobal">
      
      <!-- Connectivity constraint applied to each cell. Energy penalty specifies how strong the constraint is. Penalty is specified for each type  -->
      <!-- This constraint works in 2D and 3D on all type of lattices. It might be slowdown your simulation. For faster option - 2D and square lattice you may use Connectivity or ConnectivityLocalFlex -->
      <!-- To speed up simulation comment out unnecessary constraints for types which don't need the constraint -->
      <!-- By default we will always precheck connectivity BUT in simulations in which there is no risk of having unfragmented cell one can add this flag to speed up computations -->
      <!-- To turn off precheck uncomment line below -->
      <!-- <DoNotPrecheckConnectivity/> -->
      <Penalty Type="P">1000000</Penalty>
      <Penalty Type="N">1000000</Penalty>
      <Penalty Type="EC">1000000</Penalty>
      <Penalty Type="NV">1000000</Penalty>
   </Plugin>
   
   <Steppable Type="DiffusionSolverFE">
      
      <!-- Specification of PDE solvers -->
      <DiffusionField Name="S_VEGF">
         <DiffusionData>
            <FieldName>S_VEGF</FieldName>
            <GlobalDiffusionConstant>0.016</GlobalDiffusionConstant>
            <GlobalDecayConstant>0.0016</GlobalDecayConstant>
            <DoNotDecayIn>EC</DoNotDecayIn>
            <DoNotDecayIn>NV</DoNotDecayIn>
            <InitialConcentrationExpression>0.0</InitialConcentrationExpression>
         </DiffusionData>
         <SecretionData>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="Medium"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="P"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="N"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="EC"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="NV"/>
            <!-- When secretion is defined inside DiffusionSolverFE all secretion constants are scaled automatically to account for the extra calls to the diffusion step when handling large diffusion constants -->
            
            <!-- Uniform secretion Definition -->
            <!-- <Secretion Type="P">0.1</Secretion> -->
            <!-- <Secretion Type="N">0.1</Secretion> -->
            <Secretion Type="EC">0.0013</Secretion>
            <Secretion Type="NV">0.0013</Secretion>
            <!-- <SecretionOnContact SecreteOnContactWith="P,N,EC,NV" Type="NV">0.2</SecretionOnContact> -->
            <!-- <ConstantConcentration Type="NV">0.1</ConstantConcentration> -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Z">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
      <DiffusionField Name="L_VEGF">
         <DiffusionData>
            <FieldName>L_VEGF</FieldName>
            <GlobalDiffusionConstant>0.16</GlobalDiffusionConstant>
            <GlobalDecayConstant>0.0016</GlobalDecayConstant>
            <InitialConcentrationExpression>0.0</InitialConcentrationExpression>
         </DiffusionData>
         <SecretionData>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="Medium"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="P"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="N"/>
            <Uptake MaxUptake="0.05" RelativeUptakeRate="0.5" Type="EC"/>
            <Uptake MaxUptake="0.05" RelativeUptakeRate="0.5" Type="NV"/>
            <!-- When secretion is defined inside DiffusionSolverFE all secretion constants are scaled automatically to account for the extra calls to the diffusion step when handling large diffusion constants -->
            
            <!-- Uniform secretion Definition -->
            <Secretion Type="P">0.001</Secretion>
            <!-- <Secretion Type="N">0.1</Secretion> -->
            <!-- <Secretion Type="EC">0.1</Secretion> -->
            <!-- <Secretion Type="NV">0.1</Secretion> -->
            <!-- <SecretionOnContact SecreteOnContactWith="P,N,EC,NV" Type="NV">0.2</SecretionOnContact> -->
            <!-- <ConstantConcentration Type="NV">0.1</ConstantConcentration> -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Z">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
      <DiffusionField Name="GLU">
         <DiffusionData>
            <FieldName>GLU</FieldName>
            <GlobalDiffusionConstant>0.16</GlobalDiffusionConstant>
            <GlobalDecayConstant>0.0001</GlobalDecayConstant>
         </DiffusionData>
         <SecretionData>
            <Uptake MaxUptake="0.0064" RelativeUptakeRate="0.1" Type="Medium"/>
            <Uptake MaxUptake="0.1" RelativeUptakeRate="0.1" Type="P"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="N"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="EC"/>
            <Uptake MaxUptake="0.0" RelativeUptakeRate="0.0" Type="NV"/>
            <!-- When secretion is defined inside DiffusionSolverFE all secretion constants are scaled automatically to account for the extra calls to the diffusion step when handling large diffusion constants -->
            
            <!-- Uniform secretion Definition -->
            <!-- <Secretion Type="P">0.1</Secretion> -->
            <!-- <Secretion Type="N">0.1</Secretion> -->
            <Secretion Type="EC">0.8</Secretion>
            <Secretion Type="NV">0.4</Secretion>
            <!-- <SecretionOnContact SecreteOnContactWith="P,N,EC,NV" Type="NV">0.2</SecretionOnContact> -->
            <!-- <ConstantConcentration Type="NV">0.1</ConstantConcentration> -->
         </SecretionData>
         <BoundaryConditions>
            <Plane Axis="X">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Y">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
            <Plane Axis="Z">
               <ConstantDerivative PlanePosition="Min" Value="0.0"/>
               <ConstantDerivative PlanePosition="Max" Value="0.0"/>
            </Plane>
         </BoundaryConditions>
      </DiffusionField>
   </Steppable>
   
   <Steppable Type="UniformInitializer">
      
